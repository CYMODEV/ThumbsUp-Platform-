├─ packages/
│  ├─ sdk-web/           # JS/TS web SDK
│  ├─ sdk-mobile/        # iOS/Android bindings
│  ├─ sdk-pos/           # POS connectors (chip/RF/IR & receipt ThumbCode)
│  ├─ token-core/        # Token ledger adapters & attestations
│  ├─ wallet-core/       # Key mgmt, signing, custody modes
│  └─ analytics/         # Event schemas, dashboards, ETL
├─ services/
│  ├─ attestation/       # Validate ThumbCodes/events, anti‑fraud
│  ├─ settlement/        # Merchant reconciliation & payouts
│  ├─ campaigns/         # Sponsored thumbs & loyalty budgets
│  └─ api-gateway/       # Public REST/GraphQL interface
├─ apps/
│  ├─ mobile/            # React Native / SwiftUI / Kotlin
│  ├─ dashboard/         # Merchant & creator analytics
│  └─ kiosk/             # RingMint/receipt kiosks (optional)
├─ hardware/
│  ├─ firmware/          # Secure element, biometrics, transport
│  └─ reference/         # PCB, enclosure, testing
├─ .github/
│  ├─ ISSUE_TEMPLATE/
│  │  ├─ bug_report.md
